tipo_bienes.descripcion,
  salida.consecutivo consecutivo_salida,
  grupo_cuentasalida,
  grupo_cuentaentrada,
  salida.fecha_registro,
    ( 
    'Entrada No. '||entrada.consecutivo ||' del '|| entrada.fecha_registro||', Factura No.'||
   entrada.numero_factura||' del '||
   fecha_factura) concepto,  
   &quot;ESF_NOMBRE_ESPACIO&quot; nombre_dependencia,
&quot;ESF_ID_ESPACIO&quot; cod_dependencia,
  total_iva_con valor_total,
  &quot;FUN_IDENTIFICACION&quot; funcionario_identificacion, 
  &quot;FUN_NOMBRE&quot; funcionario_nombre, salida.vigencia
 FROM entrada 
  JOIN clase_entrada ON clase_entrada.id_clase=entrada.clase_entrada
  JOIN elemento ON elemento.id_entrada=entrada.id_entrada
  JOIN elemento_individual ON elemento_individual.id_elemento_gen=elemento.id_elemento
  JOIN salida ON salida.id_entrada=entrada.id_entrada
  JOIN tipo_bienes ON elemento.tipo_bien=id_tipo_bienes
  JOIN grupo.grupo_descripcion ON grupo.grupo_descripcion.grupo_id=cast(nivel as character varying)
 JOIN arka_parametros.arka_funcionarios ON arka_parametros.arka_funcionarios.&quot;FUN_IDENTIFICACION&quot;=salida.funcionario
 JOIN arka_parametros.arka_espaciosfisicos ON arka_parametros.arka_espaciosfisicos.&quot;ESF_ID_ESPACIO&quot;=salida.dependencia
 WHERE 
 elemento.id_entrada=salida.id_entrada
 AND salida.funcionario=arka_parametros.arka_funcionarios.&quot;FUN_IDENTIFICACION&quot;
 AND entrada.id_entrada=salida.id_entrada
 AND elemento_individual.id_salida=salida.id_salida</SQLRaw></SQL><QueryColumns><QueryColumn><Name>fecha_registro</Name><TableName>salida</TableName><ColumnName>fecha_registro</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>consecutivo_salida</Name><TableName>salida</TableName><ColumnName>consecutivo</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>grupo_cuentasalida</Name><TableName></TableName><ColumnName>grupo_cuentasalida</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>grupo_cuentaentrada<
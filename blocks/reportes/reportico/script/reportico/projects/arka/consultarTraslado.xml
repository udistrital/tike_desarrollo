historial_elemento_individual.fecha_registro,
placa,
   &quot;ESF_NOMBRE_ESPACIO&quot; dependencia,
pasado.&quot;FUN_NOMBRE&quot; as funcionario_anterior,
actual.&quot;FUN_NOMBRE&quot; as funcionario_siguiente,
marca, 
elemento_individual.serie,
elemento_individual.observaciones_traslados,
elemento_nombre as nombre_nivel
FROM elemento_individual
JOIN elemento ON id_elemento_gen = elemento.id_elemento
JOIN historial_elemento_individual ON historial_elemento_individual.elemento_individual = elemento_individual.id_elemento_ind 
JOIN salida ON salida.id_entrada = elemento.id_entrada
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=nivel
JOIN arka_parametros.arka_funcionarios actual ON actual.&quot;FUN_IDENTIFICACION&quot;=salida.funcionario
JOIN arka_parametros.arka_funcionarios pasado ON pasado.&quot;FUN_IDENTIFICACION&quot;=historial_elemento_individual.funcionario
 JOIN arka_parametros.arka_espaciosfisicos ON arka_parametros.arka_espaciosfisicos.&quot;ESF_ID_ESPACIO&quot;=salida.dependencia
JOIN catalogo.catalogo_lista ON catalogo.catalogo_elemento.elemento_catalogo=catalogo.catalogo_lista.lista_id
WHERE elemento_individual.id_salida=salida.id_salida
ORDER BY historial_elemento_individual.fecha_registro ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>marca</Name><TableName></TableName><ColumnName>marca</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>dependencia</Name><TableName></TableName><ColumnName>&quot;ESF_NOMBRE_ESPACIO&quot;</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>funcionario_anterior</Name><TableName></TableName><ColumnName>pasado.&quot;FUN_NOMBRE&quot; as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>funcionario_sigu
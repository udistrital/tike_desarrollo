    &quot;ESF_NOMBRE_ESPACIO&quot; nombre_dependencia
FROM  arka_parametros.arka_funcionarios,elemento
JOIN elemento_individual ON elemento.id_elemento=elemento_individual.id_elemento_gen  
JOIN salida ON elemento_individual.id_salida=salida.id_salida
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=elemento.nivel

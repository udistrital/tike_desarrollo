entrada.consecutivo entrada_consecutivo, 
salida.consecutivo salida_consecutivo,
salida.fecha_registro, 
arka_parametros.arka_espaciosfisicos.&quot;ESF_NOMBRE_ESPACIO&quot; dependenciaespacio, 
arka_parametros.arka_sedes.&quot;ESF_SEDE&quot; sede,
arka_parametros.arka_funcionarios.&quot;FUN_NOMBRE&quot; nombrefun, arka_parametros.arka_funcionarios.&quot;FUN_IDENTIFICACION&quot; idfun,
elemento_nombre nivel,
elemento_individual.placa,
elemento_individual.serie,
salida.vigencia
FROM salida 
JOIN entrada ON salida.id_entrada=entrada.id_entrada 
JOIN arka_parametros.arka_funcionarios ON salida.funcionario=arka_parametros.arka_funcionarios.&quot;FUN_IDENTIFICACION&quot;
JOIN arka_parametros.arka_espaciosfisicos ON cast(salida.dependencia as character varying)=arka_parametros.arka_espaciosfisicos.&quot;ESF_ID_ESPACIO&quot;
JOIN arka_parametros.arka_sedes ON salida.sede=cast(arka_parametros.arka_sedes.&quot;ESF_ID_SEDE&quot; as character varying)
JOIN elemento_individual ON elemento_individual.id_salida=salida.id_salida
JOIN elemento on elemento_individual.id_elemento_gen=elemento.id_elemento
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=elemento.nivel
ORDER BY salida.id_salida ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>entrada_consecutivo</Name><TableName>entrada</TableName><ColumnName>consecutivo</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>salida_consecutivo</Name><TableName>salida</TableName><ColumnName>consecutivo</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>fecha_registro</Name><TableName>salida</TableName><ColumnName>fecha_registro</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>dependenciaespacio</Name><TableName></Tab
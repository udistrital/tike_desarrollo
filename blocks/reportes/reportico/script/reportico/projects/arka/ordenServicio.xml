<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Orden de Servicio</ReportTitle><ReportDescription>Seleccione los criterios de b√∫squeda</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin>1cm</BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>no</gridSearchable><gridPageable>no</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>none</DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT id_orden_servicio,orden_servicio.fecha_registro,
arka_parametros.arka_sedes.&quot;ESF_SEDE&quot;  sede,
rubro,arka_parametros.arka_espaciosfisicos.&quot;ESF_NOMBRE_ESPACIO&quot; dependencia, 
total,arka_parametros.arka_proveedor.&quot;PRO_RAZON_SOCIAL&quot; contratista, arka_parametros.arka_ordenadores.&quot;ORG_IDENTIFICACION&quot; id_ordenador,arka_parametros.arka_ordenadores.&quot;ORG_NOMBRE&quot; ordenador,
duracion_pago, fecha_inicio_pago, fecha_final_pago,
objeto_contrato, forma_pago, 
total_preliminar, iva, entrada.vigencia
FROM orden_servicio 
JOIN arka_parametros.arka_espaciosfisicos ON arka_parametros.arka_espaciosfisicos.&quot;ESF_ID_ESPACIO&quot; = orden_servicio.dependencia_solicitante
JOIN arka_parametros.arka_sedes ON arka_parametros.arka_sedes.&quot;ESF_ID_SEDE&quot; = orden_servicio.sede
JOIN arka_parametros.arka_ordenadores ON arka_parametros.arka_ordenadores.&quot;ORG_IDENTIFICADOR&quot; = orden_servicio.id_ordenador_encargado
JOIN informacion_presupuestal_orden ON informacion_presupuestal_orden.id_informacion=orden_servicio.info_presupuestal 
JOIN arka_parametros.arka_proveedor ON arka_parametros.arka_proveedor.&quot;PRO_NIT&quot;=CAST(id_contratista as character varying)
JOIN registro_actarecibido ON registro_actarecibido.numero_orden=id_orden_servicio
JOIN tipo_orden ON tipo_orden.to_id=registro_actarecibido.tipo_orden
JOIN entrada ON entrada.acta_recibido=registro_actarecibido.id_actarecibido
WHERE 1=1 
AND orden_servicio.estado=TRUE
AND tipo_orden.to_id=1
AND arka_parametros.arka_ordenadores.&quot;ORG_ESTADO&quot;='A'</SQLRaw></SQL><QueryColumns><QueryColumn><Name>id_orden_servicio</Name><TableName></TableName><ColumnName>id_orden_servicio</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>fecha_registro</Name><TableName>orden_servicio</TableName><ColumnName>fecha_registro</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>sede</Name><TableName></TableName><ColumnName>arka_parametros.arka_sedes.&quot;ESF_SEDE&quot; </ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><Que
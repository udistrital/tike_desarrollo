<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Consultar Inventarios Asignados a Contratistas</ReportTitle><ReportDescription>Seleccione los criterios correspondientes para consultar los elementos asignados a uno o varios contratistas</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin>1cm</BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>no</gridSearchable><gridPageable>no</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>none</DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT descripcion,elemento_nombre nivel, marca, 
elemento_individual.placa,elemento_individual.serie, 
arka_parametros.arka_contratistas.&quot;CON_NOMBRE&quot; as contratista_nombre, 
arka_parametros.arka_contratistas.&quot;CON_IDENTIFICACION&quot; as contratista_identificacion,
&quot;ESF_NOMBRE_ESPACIO&quot; nombre_dependencia
FROM arka_parametros.arka_contratistas,asignar_elementos, elemento
JOIN elemento_individual ON elemento.id_elemento=elemento_individual.id_elemento_gen 
JOIN salida on elemento_individual.id_salida=salida.id_salida
JOIN arka_parametros.arka_espaciosfisicos ON arka_parametros.arka_espaciosfisicos.&quot;ESF_ID_ESPACIO&quot;=salida.dependencia
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=elemento.nivel
WHERE elemento_individual.estado_registro=TRUE  
AND asignar_elementos.contratista=arka_parametros.arka_contratistas.&quot;CON_IDENTIFICACION&quot;
AND elemento_individual.id_elemento_ind=asignar_elementos.id_elemento  
AND elemento_individual.estado_asignacion=TRUE  
AND asignar_elementos.estado=1
AND salida.id_entrada=elemento.id_entrada
AND salida.id_salida=elemento_individual.id_salida</SQLRaw></SQL><QueryColumns><QueryColumn><Name>marca</Name><TableName></TableName><ColumnName>marca</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>nivel</Name><TableName></TableName><ColumnName>elemento_nombre</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>serie</Name><TableName>elemento_individual</TableName><ColumnName>serie</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>placa</Name><TableName>elemento_individual</TableName><ColumnName>p
    &quot;ESF_NOMBRE_ESPACIO&quot; nombre_dependencia,
    &quot;ESF_ID_ESPACIO&quot; cod_dependencia
FROM  arka_parametros.arka_funcionarios,elemento
JOIN elemento_individual ON elemento.id_elemento=elemento_individual.id_elemento_gen  
JOIN salida ON elemento_individual.id_salida=salida.id_salida
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=elemento.nivel
JOIN arka_parametros.arka_espaciosfisicos ON arka_parametros.arka_espaciosfisicos.&quot;ESF_ID_ESPACIO&quot;=salida.dependencia
WHERE  elemento_individual.estado_registro=TRUE  
AND salida.funcionario=arka_parametros.arka_funcionarios.&quot;FUN_IDENTIFICACION&quot;
AND salida.id_salida=elemento_individual.id_salida 
AND salida.id_entrada=elemento.id_entrada
order by cod_dependencia,nombre_dependencia
</SQLRaw><SQL><QuerySql></QuerySql></SQL><QueryColumns><QueryColumn><Name>nombre_dependencia</Name><TableName></TableName><ColumnName>DISTINCT

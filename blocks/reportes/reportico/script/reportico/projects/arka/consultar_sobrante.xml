<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Consultar Sobrantes</ReportTitle><ReportDescription>Seleccione los criterios de b√∫squeda</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin>1cm</BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>no</gridSearchable><gridPageable>no</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>none</DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
nivel,
catalogo_elemento.elemento_nombre as nivel_nombre, 
id_sobrante,
estado_elemento.fecha_registro,
elemento_individual.placa, 
marca,
elemento_individual.serie,
total_iva,
total_iva_con,
salida.vigencia,
   &quot;ESF_NOMBRE_ESPACIO&quot; nombre_dependencia,
  &quot;FUN_IDENTIFICACION&quot; funcionario_identificacion, 
  &quot;FUN_NOMBRE&quot; funcionario_nombre
  FROM estado_elemento
  JOIN tipo_falt_sobr ON tipo_faltsobr=id_tipo_falt_sobr
  JOIN elemento_individual ON elemento_individual.id_elemento_ind=estado_elemento.id_elemento_ind
  JOIN elemento ON id_elemento_gen=id_elemento
  JOIN salida ON salida.id_entrada=elemento.id_entrada
  JOIN entrada ON entrada.id_entrada=elemento.id_entrada
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=elemento.nivel
JOIN catalogo.catalogo_lista ON catalogo.catalogo_elemento.elemento_catalogo=catalogo.catalogo_lista.lista_id
 JOIN arka_parametros.arka_funcionarios ON arka_parametros.arka_funcionarios.&quot;FUN_IDENTIFICACION&quot;=salida.funcionario
 JOIN arka_parametros.arka_espaciosfisicos ON arka_parametros.arka_espaciosfisicos.&quot;ESF_ID_ESPACIO&quot;=salida.dependencia
  WHERE tipo_faltsobr=1
  AND salida.id_salida=elemento_individual.id_salida
  AND estado_elemento.estado_registro='t'</SQLRaw></SQL><QueryColumns><QueryColumn><Name>nivel</Name><TableName></TableName><ColumnName>nivel</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>nivel_nombre</Name><TableName></TableName><ColumnName>catalogo_elemento.elemento_nombre as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>id_sobrante</Name><TableName></TableName><ColumnName>id_sobrante</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>fecha_registro</Name><TableName>estado_elemento</TableN
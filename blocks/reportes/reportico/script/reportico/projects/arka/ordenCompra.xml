<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Orden de Compra</ReportTitle><ReportDescription>Seleccione los criterios de b√∫squeda</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin>1cm</BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>no</gridSearchable><gridPageable>no</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>none</DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT id_orden_compra, orden_compra.fecha_registro,&quot;PRO_NIT&quot; proveedor , &quot;PRO_RAZON_SOCIAL&quot; proveedor_nombre,&quot;ESF_NOMBRE_ESPACIO&quot; dependencia, rubro,vigencia_dispo, 
numero_dispo,vigencia_regis,numero_regis, lugar_entrega, supervision,
 arka_parametros.arka_ordenadores.&quot;ORG_IDENTIFICACION&quot; id_ordenador,arka_parametros.arka_ordenadores.&quot;ORG_NOMBRE&quot; ordenador_nombre, total , entrada.vigencia
FROM orden_compra
JOIN informacion_presupuestal_orden ON informacion_presupuestal_orden.id_informacion=orden_compra.info_presupuestal
JOIN arka_parametros.arka_proveedor ON arka_parametros.arka_proveedor.&quot;PRO_NIT&quot; = cast(orden_compra.id_proveedor as character varying)
JOIN arka_parametros.arka_espaciosfisicos ON arka_parametros.arka_espaciosfisicos.&quot;ESF_ID_ESPACIO&quot; = orden_compra.id_dependencia 
JOIN registro_actarecibido ON registro_actarecibido.numero_orden=id_orden_compra
JOIN tipo_orden ON tipo_orden.to_id=registro_actarecibido.tipo_orden
JOIN entrada ON entrada.acta_recibido=registro_actarecibido.id_actarecibido
JOIN arka_parametros.arka_ordenadores ON arka_parametros.arka_ordenadores.&quot;ORG_IDENTIFICADOR&quot; = orden_compra.id_ordenador
WHERE 1=1 
AND orden_compra.estado=TRUE
AND tipo_orden.to_id=2
AND arka_parametros.arka_ordenadores.&quot;ORG_ESTADO&quot;='A'</SQLRaw></SQL><QueryColumns><QueryColumn><Name>id_orden_compra</Name><TableName></TableName><ColumnName>id_orden_compra</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>fecha_registro</Name><TableName>orden_compra</TableName><ColumnName>fecha_registro</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>proveedor</Name><TableName></TableName><ColumnName>&quot;PRO_NIT&quot;</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>proveedor_nombr